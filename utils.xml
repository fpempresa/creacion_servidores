<?xml version="1.0" encoding="UTF-8"?><project basedir="." name="utils">	<target name="show-jenkins-password" depends="-configureAnt">		<!-- Mostrar el usuario y la contraseÃ±a de Jenkins -->				<applicationproperty     			userName="${OPENSHIFT_DESARROLLO_LOGIN}" 			password="${OPENSHIFT_DESARROLLO_PASSWORD}"  			domainName="${DESARROLLO_DOMAIN}" 			applicationName="${JENKINS_APPLICATION}"  			name="SSH_HOST" 			applicationProperty="SSHHost"		/>		<applicationproperty     			userName="${OPENSHIFT_DESARROLLO_LOGIN}" 			password="${OPENSHIFT_DESARROLLO_PASSWORD}"  			domainName="${DESARROLLO_DOMAIN}" 			applicationName="${JENKINS_APPLICATION}"  			name="SSH_USER" 			applicationProperty="SSHUser"		/>		<applicationproperty     			userName="${OPENSHIFT_DESARROLLO_LOGIN}" 			password="${OPENSHIFT_DESARROLLO_PASSWORD}"  			domainName="${DESARROLLO_DOMAIN}" 			applicationName="${JENKINS_APPLICATION}"  			name="SSH_PORT" 			applicationProperty="SSHPort"		/>						<echo>Usuario de Jenkins</echo>		<sshexec 			host="${SSH_HOST}"			username="${SSH_USER}"			port="${SSH_PORT}"			keyfile="${DESARROLLO_PRIVATE_KEY}"			command="echo $JENKINS_USERNAME" trust="true"		/>				<echo>Contrasenya de Jenkins</echo>		<sshexec 			host="${SSH_HOST}"			username="${SSH_USER}"			port="${SSH_PORT}"			keyfile="${DESARROLLO_PRIVATE_KEY}"			command="echo $JENKINS_PASSWORD" trust="true"		/>			</target>		<target name="show-jenkins-sshurl" depends="-configureAnt">		<!-- Mostrar la url de conexion de ssh de jenkins -->				<applicationproperty     			userName="${OPENSHIFT_DESARROLLO_LOGIN}" 			password="${OPENSHIFT_DESARROLLO_PASSWORD}"  			domainName="${DESARROLLO_DOMAIN}" 			applicationName="${JENKINS_APPLICATION}"  			name="SSH_URL" 			applicationProperty="SshUrl"		/>				<echo>SSH URL del domino ${DESARROLLO_DOMAIN} en aplicacion ${JENKINS_APPLICATION}</echo>		<echo>${SSH_URL}</echo>	</target>	<target name="show-produccion-sshurl" depends="-configureAnt">		<!-- Mostrar la url de conexion de ssh de jenkins -->				<applicationproperty     			userName="${OPENSHIFT_PRODUCCION_LOGIN}" 			password="${OPENSHIFT_PRODUCCION_PASSWORD}"  			domainName="${PRODUCCION_DOMAIN}" 			applicationName="${PRODUCCION_APPLICATION}"  			name="SSH_URL" 			applicationProperty="SshUrl"		/>				<echo>SSH URL del domino ${PRODUCCION_DOMAIN} en aplicacion ${PRODUCCION_APPLICATION}</echo>		<echo>${SSH_URL}</echo>			</target>	</project>